
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - B2B Tecnology</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/css/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/sb-admin.css" rel="stylesheet" />
    <link href="~/css/plugins/morris.css" rel="stylesheet" />
    <link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/B2BTecnology.css" rel="stylesheet" />

</head>
<body>

    @RenderSection("BeginBody", required: false)

    <div id="wrapper">

        <div id="page-wrapper">
            <div class="container-fluid">

                @RenderBody()

            </div>
        </div>

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" href="/Home">B2B Technology</a>
            </div>
            <!-- Top Menu Items -->
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#demo"><i class="fa fa-dashboard fa-fw"></i> Clientes <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="demo" class="collapse">
                            <li>
                                <a href="/Cliente">Detalhes</a>
                            </li>
                            <li>
                                <a href="/Cliente/Listar">Listar</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="/Vendedores"><i class="fa fa-bar-chart-o fa-fw"></i> Vendedores</a>
                    </li>
                    <li>
                        <a href="/Equipamentos"><i class="fa fa-table fa-fw"></i> Equipamentos</a>
                    </li>
                    <li>
                        <a href="/Comissao"><i class="fa fa-edit fa-fw"></i> Comissão</a>
                    </li>
                    <li>
                        <a href="/Pagamento"><i class="fa fa-wrench fa-fw"></i> Pagamentos</a>
                    </li>
                </ul>
            </div>
        </nav>



    </div>


    <footer id="Footer" class="hidden-print">
        <hr />
        @RenderSection("actions", required: false)
    </footer>

    @RenderSection("EndBody", required: false)

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/js/jquery.js"></script>
    <script src="~/js/jquery-1.12.2.min.js"></script>
    <script src="~/js/jquery-ui.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/Numeral/numeral.min.js"></script>
    @*<script src="~/js/plugins/morris/morris.min.js"></script>*@
    <script src="~/js/plugins/morris/raphael.min.js"></script>
    @*<script src="~/js/plugins/morris/morris-data.js"></script>*@
    <script src="~/js/Mascara/jquery.mask.min.js"></script>
    <script src="~/js/B2BTecnology.js"></script>
</body>
</html>
